body { font-family: 'Open Sans', sans-serif;}
footer .col h5 {
  color: var(--pwb-footer-text-color);
}

footer .col p {
  color: var(--pwb-footer-text-color);
}

.footer-extra {
  color: var(--pwb-footer-text-color);
}

footer .col ul li {
  color: var(--pwb-footer-text-color);
}

.fondo_accion {
  background: var(--pwb-primary-color);
  color: #fff;
}


.services-section .bst-container .services-wrapper .single-service .bordered {
  border: 2px solid var(--pwb-primary-color);
}
.services-section .bst-container .services-wrapper .single-service .icon-service {
  color: var(--pwb-primary-color);
  border-color: var(--pwb-primary-color);
}
.services-section .bst-container .services-wrapper .single-service:hover h4 {
  color: var(--pwb-primary-color);
}


