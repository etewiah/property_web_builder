<div id="resp_buscar_inmuebles">
  <ul class="list-listings" id="ordered-properties">
    <% unless @properties && (@properties.size > 0 ) %>
      <div class="text-center" style="padding: 40px 0;">
        <i class="fa fa-search fa-4x text-muted" style="margin-bottom: 20px;"></i>
        <h4 style="text-transform: none; margin-bottom: 20px;">
          <%= t "noResultsForSearch" %>
        </h4>
        <p class="text-muted">
          <%= t "tryChangingFilters", default: "Try changing your filters" %>
        </p>
        <button class="btn btn-primary" onclick="$('form.form-light')[0].reset(); $('select').selectpicker('refresh'); $('form.form-light').submit();">
          <i class="fa fa-refresh"></i> <%= t "clearFilters", default: "Clear Filters" %>
        </button>
      </div>
    <% end %>
    <% @properties.each_with_index do |property, index| %>
      <%= render :partial => '/pwb/search/search_result_item', :locals => { property: property, index: index } %>
    <% end %>
  </ul>
  <!-- PAGINATION & FILTERS -->
  <div class="wp-block default product-list-filters light-gray" style="display:none;">
    <ul class="pagination pagination pull-left">
      <li><a href="#" hidefocus="true">«</a></li>
      <li class="active"><a href="#" hidefocus="true">1</a></li>
      <li><a href="#" hidefocus="true">2</a></li>
      <li><a href="#" hidefocus="true">3</a></li>
      <li><a href="#" hidefocus="true">4</a></li>
      <li><a href="#" hidefocus="true">5</a></li>
      <li><a href="#" hidefocus="true">»</a></li>
    </ul>
  </div>
</div>
