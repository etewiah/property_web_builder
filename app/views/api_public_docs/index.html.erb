<!DOCTYPE html>
<html>
  <head>
    <title>API Public V1 Docs</title>
    <meta charset="UTF-8">
    <link rel="stylesheet" type="text/css" href="/api-docs/swagger-ui.css">
    <link rel="icon" type="image/png" href="/api-docs/favicon-32x32.png" sizes="32x32" />
    <link rel="icon" type="image/png" href="/api-docs/favicon-16x16.png" sizes="16x16" />
    <style>
      html
      {
        box-sizing: border-box;
        overflow: -moz-scrollbars-vertical;
        overflow-y: scroll;
      }

      *,
      *:before,
      *:after
      {
        box-sizing: inherit;
      }

      body
      {
        margin:0;
        background: #fafafa;
      }
    </style>
  </head>

  <body>
    <div id="swagger-ui"></div>

    <script src="/api-docs/swagger-ui-bundle.js"> </script>
    <script src="/api-docs/swagger-ui-standalone-preset.js"> </script>
    <script>
    window.onload = function() {
      // Begin Swagger UI call region
      const ui = SwaggerUIBundle({
        url: "/api-docs/v1/api_public_swagger.yaml",
        dom_id: '#swagger-ui',
        deepLinking: true,
        presets: [
          SwaggerUIBundle.presets.apis,
          SwaggerUIStandalonePreset
        ],
        plugins: [
          SwaggerUIBundle.plugins.DownloadUrl
        ],
        layout: "StandaloneLayout"
      })
      // End Swagger UI call region

      window.ui = ui
    }
  </script>
  </body>
</html>
